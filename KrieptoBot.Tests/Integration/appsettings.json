{
  "IsEncrypted": false,
  "Values": {
    "AzureWebJobsStorage": "UseDevelopmentStorage=true",
    "FUNCTIONS_WORKER_RUNTIME": "dotnet-isolated"
  },
  "Secrets": {
    "BitvavoConfig": {
      "ApiKey": "{APIKEY}",
      "ApiSecret": "{APISECRET}",
      "BaseUrl": ""
    }
  },
  "RecommendatorSettings": {
    "BuyRecommendationWeights": {
      "RecommendatorRsi14PeriodInterval": "1",
      "RecommendatorRsi14Period4H": "1",
      "RecommendatorProfitPercentage": "1",
      "RecommendatorMacd": "1",
      "RecommendatorDownTrend": "1",
      "RecommendatorRsiEma": "1"
    },
    "SellRecommendationWeights": {
      "RecommendatorRsi14PeriodInterval": "1",
      "RecommendatorRsi14Period4H": "1",
      "RecommendatorProfitPercentage": "1",
      "RecommendatorMacd": "1",
      "RecommendatorDownTrend": "1",
      "RecommendatorRsiEma": "1"
    }
  },
  "TradingSettings": {
    "MarketsToWatch": [
      "CHZ-EUR",
      "BTC-EUR",
      "ADA-EUR",
      "HOT-EUR",
      "1INCH-EUR",
      "ETH-EUR",
      "DOGE-EUR",
      "SHIB-EUR",
      "SOL-EUR",
      "MANA-EUR"
    ],
    "Interval": "5m",
    "BuyMargin": 30,
    "SellMargin": -30,
    "SimulationMode": true,
    "MaxBuyBudgetPerCoin": "50",
    "MinBuyBudgetPerCoin": "20",
    "PollingIntervalInMinutes": "5",
    "BuyCoolDownPeriodInMinutes": "1"
  }
}
