{
  "Serilog": {
    "MinimumLevel": "Information",
    "WriteTo": [
      "Console"
    ]
  },
  "Secrets": {
    "BitvavoConfig": {
      "ApiKey": "{APIKEY}",
      "ApiSecret": "{APISECRET}",
      "BaseUrl": "https://api.bitvavo.com"
    }
  },
  "RecommendatorSettings": {
    "BuyRecommendationWeights": {
      "RecommendatorSupport": "0",
      "RecommendatorRsi14PeriodInterval": "1",
      "RecommendatorRsi14Period4H": "1",
      "RecommendatorProfitPercentage": "1",
      "RecommendatorMacd": "2"
    },
    "SellRecommendationWeights": {
      "RecommendatorSupport": "0",
      "RecommendatorRsi14PeriodInterval": "1",
      "RecommendatorRsi14Period4H": "1",
      "RecommendatorProfitPercentage": "1",
      "RecommendatorMacd": "2"
    }
  },
  "TradingSettings": {
    "MarketsToWatch": [
      "BTC-EUR"
    ],
    "Interval": "5m",
    "BuyMargin": 30,
    "SellMargin": -30,
    "SimulationMode": true
  }
}
