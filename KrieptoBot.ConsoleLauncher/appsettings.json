{
  "Serilog": {
    "MinimumLevel": "Debug",
    "WriteTo": [
      "Console"
    ]
  },
  "Secrets": {
    "BitvavoConfig": {
      "ApiKey": "{APIKEY}",
      "ApiSecret": "{APISECRET}",
      "BaseUrl": "https://api.bitvavo.com"
    }
  },
  "RecommendatorSettings": {
    "BuyRecommendationWeights": {
      "RecommendatorRsi14PeriodInterval": "1",
      "RecommendatorRsi14Period4H": "1",
      "RecommendatorProfitPercentage": "1",
      "RecommendatorMacd": "1.5"
    },
    "SellRecommendationWeights": {
      "RecommendatorRsi14PeriodInterval": "1",
      "RecommendatorRsi14Period4H": "1",
      "RecommendatorProfitPercentage": "2",
      "RecommendatorMacd": "1.5"
    },
    "ProfitRecommendatorProfitThresholdPct": "2",
    "ProfitRecommendatorLossThresholdPct": "1.5"
  },
  "TradingSettings": {
    "MarketsToWatch": [
      "BTC-EUR",
      "CHZ-EUR",
      "ADA-EUR",
      "HOT-EUR",
      "1INCH-EUR",
      "ETH-EUR",
      "DOGE-EUR",
      "SHIB-EUR",
      "SOL-EUR",
      "MANA-EUR",
      "XRP-EUR"
    ],
    "Interval": "5m",
    "BuyMargin": 1.5,
    "SellMargin": -1.5,
    "IsSimulation": true,
    "MaxBuyBudgetPerCoin": "50",
    "MinBuyBudgetPerCoin": "20"
  }
}
